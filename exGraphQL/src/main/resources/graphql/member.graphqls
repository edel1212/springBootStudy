schema {
    query: Query,
    mutation: Mutation,
}

type Mutation {
    registerMember(memberEntity : MemberEntity): Member
    updateMember(memberEntity : MemberEntity): Boolean
    deleteMember(email: String!): Boolean
}

type Query{
	findMember(email: String!): Member
	allFindMembers: [Member]
}


# 응답 : member
type Member{
	email: String
	password: String
	name : String
	regDate : String
	modDate : String
}

input MemberEntity{
	email: String
	password: String
	name : String
}